{
	"name": "Lüüdi",
	"version": "2025-08-01",
	"author": [
		"Niklas Laxström"
	],
	"url": "https://sanat.csc.fi/wiki/L%C3%BC%C3%BCdi",
	"descriptionmsg": "sanat-lud-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"AutoloadNamespaces": {
		"MediaWiki\\Extensions\\Lud\\": "src/"
	},
	"TestAutoloadNamespaces": {
		"MediaWiki\\Extensions\\Lud\\": "phpunit/"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extensions\\Lud\\Hooks"
		}
	},
	"Hooks": {
		"BeforePageDisplay": "main",
		"ParserFirstCallInit": "main"
	},
	"MessagesDirs": {
		"Lud": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"Lud-Magic": "MagicWords.i18n.php"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "Lud"
	},
	"ResourceModules": {
		"ext.lud.styles": {
			"styles": "resources/ext.lud.less",
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"manifest_version": 2
}
